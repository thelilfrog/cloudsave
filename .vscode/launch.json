    "version": "0.2.0",
    "configurations": [
        {
            "name": "server",
            "type": "go",
            "request": "launch",
            "mode": "auto",
            "args": ["-document-root", "${workspaceFolder}/env"],
            "console": "integratedTerminal",
            "program": "${workspaceFolder}/cmd/server"
        },
        {
            "name": "cli",
            "type": "go",
            "request": "launch",
            "mode": "auto",
            "args": ["sync"],
            "console": "integratedTerminal",
            "program": "${workspaceFolder}/cmd/cli"
        }
    ]
}